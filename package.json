{
  "name": "amazonmonitor",
  "version": "3.0.0",
  "description": "A Discord bot that monitors Amazon items for price drops/restocks",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --coverage tests/",
    "compile": "tsc",
    "start": "yarn compile && node --experimental-modules --es-module-specifier-resolution=node built/index.js",
    "lint": "yarn eslint index.ts commands/**/*.ts common/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpikeHD/AmazonMonitor.git"
  },
  "author": "SpikeHD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SpikeHD/AmazonMonitor/issues"
  },
  "homepage": "https://github.com/SpikeHD/AmazonMonitor#readme",
  "dependencies": {
    "chalk": "^5.0.1",
    "cheerio": "1.0.0-rc.10",
    "discord.js": "^14",
    "puppeteer": "^14.3.0",
    "puppeteer-proxy": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text"
    ]
  }
}
